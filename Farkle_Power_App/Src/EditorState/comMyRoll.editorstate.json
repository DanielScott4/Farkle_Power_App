ntRows(Filter(_myRoll, Value = 2)) = 5 ||\n            CountRows(Filter(_myRoll, Value = 3)) = 5 ||\n            CountRows(Filter(_myRoll, Value = 4)) = 5 ||\n            CountRows(Filter(_myRoll, Value = 5)) = 5 ||\n            CountRows(Filter(_myRoll, Value = 6)) = 5, \n        IsFourOfAKind:\n            CountRows(Filter(_myRoll, Value = 1)) = 4 ||\n            CountRows(Filter(_myRoll, Value = 2)) = 4 ||\n            CountRows(Filter(_myRoll, Value = 3)) = 4 ||\n            CountRows(Filter(_myRoll, Value = 4)) = 4 ||\n            CountRows(Filter(_myRoll, Value = 5)) = 4 ||\n            CountRows(Filter(_myRoll, Value = 6)) = 4,\n        IsThreeOfAKind:\n            CountRows(Filter(_myRoll, Value = 1)) = 3 ||\n            CountRows(Filter(_myRoll, Value = 2)) = 3 ||\n            CountRows(Filter(_myRoll, Value = 3)) = 3 ||\n            CountRows(Filter(_myRoll, Value = 4)) = 3 ||\n            CountRows(Filter(_myRoll, Value = 5)) = 3 ||\n            CountRows(Filter(_myRoll, Value = 6)) = 3    \n    }    \n)",
